<Page
    x:Class="BShopUniversal.pgInventory"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:BShopUniversal"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="B SHOP ONLINE" VerticalAlignment="Top" Height="39" Width="293" FontSize="24"/>
        <TextBlock x:Name="textBlock1" HorizontalAlignment="Left" Margin="10,54,0,0" TextWrapping="Wrap" Text="Clothing" VerticalAlignment="Top" Height="92" Width="340" FontFamily="Segoe UI Historic" FontSize="36" SelectionChanged="textBlock1_SelectionChanged"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="390" Margin="10,128,0,0" TextWrapping="Wrap" Text="List of Clothing Inventory" VerticalAlignment="Top" Width="293"/>
        <Button x:Name="button" Content="Next" HorizontalAlignment="Left" Margin="101,561,0,0" VerticalAlignment="Top" Height="50" Width="99" Click="button_Click"/>
        <Button x:Name="button1" Content="Back" HorizontalAlignment="Left" Height="50" Margin="229,561,0,0" VerticalAlignment="Top" Width="104" Click="button1_Click"/>

    </Grid>
</Page>
